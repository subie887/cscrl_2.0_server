generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type AssociatesDocs {
  link     String
  title    String
}

model Associates {
  id        String           @id @default(auto()) @map("_id") @db.ObjectId
  bio       String
  docs      AssociatesDocs[]
  firstName String
  img       String
  lastName  String
  role      String
}

model Contacts {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Videos {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  desc      String
  eventName String
  fileName  String
  title     String
}
