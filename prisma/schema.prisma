generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type ContactsContacts {
  email String
  phone String
}

type AssociatesDocs {
  fileName String
  link     String
  title    String
}

model Associates {
  id        String           @id @default(auto()) @map("_id") @db.ObjectId
  bio       String
  docs      AssociatesDocs[]
  firstName String
  img       String
  lastName  String
  role      String
}

model Calendar {
  id    String   @id @default(auto()) @map("_id") @db.ObjectId
  date  DateTime @db.Date
  desc  String
  title String
}

model Contacts {
  id       String           @id @default(auto()) @map("_id") @db.ObjectId
  contacts ContactsContacts
  fName    String
  lName    String
  role     String
  title    String
}

model Videos {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  desc      String
  eventName String
  fileName  String
  title     String
  createdAt DateTime
}

model Lrmi {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  fileName String
  year     Int
  quarter  Int
}
